   
{
    "manifest_version": 3,
    "name": "TLDR",
    "permissions": ["storage", "activeTab", "tabs","scripting"],
    "version": "0.0.1",
    "minimum_chrome_version": "42",
    "devtools_page": "main.html",

    "icons": {
      "128": "logo/tldr.png"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
      }
  }